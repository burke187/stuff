<h1>Welcome to Stack Overflow</h1>


<%= link_to "Submit a new question?", new_question_path %><br><br>


<% @questions.each do |question| %>
<b><%= link_to question.q_title, question_path(question) %></b><br/>
<%= question.q_content %><br/>
<%= question.votes %> votes <br>
<%= distance_of_time_in_words_to_now(question.created_at) %> ago <br><br>
<% end %>

<%= form_for :session, url: login_path do  |f| %>
<%= f.text_field :email, placeholder: "Email" %>
<%= f.password_field :password, placeholder: "Password" %>
<%= f.submit %>
<% end %>
